{
    "name": "solidjs-webpack",
    "version": "0.0.1",
    "description": "Boilerplate for starting new projects with solidJs and Webpack",
    "main": "./src/index.tsx",
    "scripts": {
        "start": "pnpm run dev",
        "v": "npm-check --update",
        "test": "pnpm audit --production && pnpm run test:ut",
        "test:ut": "playwright test",
        "test:report": "playwright show-report",
        "dev": "webpack-dev-server --mode development",
        "build": "pnpm run test && webpack --mode production"
    },
